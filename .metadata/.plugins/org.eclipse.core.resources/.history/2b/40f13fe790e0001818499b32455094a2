#include<cstdio>
#include<cstring>
#include<string>
using namespace std;
int main() {
	int n;
	string s;
	scanf("%d", &n);
	for (int i=0;i<n;i++) {
		scanf("%s", s);
		if (valid(s)) printf("yes\n");
		else printf("no\n");
	}
	return 0;
}



