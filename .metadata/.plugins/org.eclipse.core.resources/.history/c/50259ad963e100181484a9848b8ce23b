#include<iostream>
#include<cstdio>
using namespace std;
int main() {
	int a[31],b[31];
	int n;
	while (scanf("%d", &n)!=EOF) {
		memset(a,0,sizeof(a));
		memset(b,0,sizeof(b));
		a[0]=1;
		for (int i=0;i<n;i++) {
			for (int j=0;j<=i;j++)
		}
	}
	return 0;
}



