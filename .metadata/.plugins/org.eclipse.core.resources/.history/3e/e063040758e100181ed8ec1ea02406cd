#include<cstdio>
using namespace std;
int gcd(int x,int y) {
	if (x==0) return y;
	else return gcd(y,x%y);
}
int main() {
	int n;
	while (scanf("%d", &n) != EOF) {
		int t, lcs=1;
		for (int i=0;i<n;i++) {
			scanf("%d", &t);
			lcs = lcs*t/gcd(lcs,t);
		}
		printf("%d\n",lcs);
	}
	return 0;
}



