#include<iostream>
#include<cstdio>
#include<string>
#include<cstring>
using namespace std;
int main() {
	int n;
	scanf("%d",&n);
	string s;
	for (int i=0;i<n;i++) {
		getline(cin,s);
		if (check(s)) printf("yes\n");
		else printf("no\n");
	}
	return 0;
}



